<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@android:color/holo_blue_light"
    tools:context="com.example.demouser.bond.IndividualContactActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="74dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/image"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/image5"
                android:layout_marginEnd="29dp"
                app:srcCompat="@drawable/ic_launcher_background" />

            <ImageView
                android:id="@+id/image4"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_alignStart="@+id/image"
                android:layout_alignTop="@+id/image3"
                app:srcCompat="@drawable/ic_launcher_background" />

            <ImageView
                android:id="@+id/image3"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_alignLeft="@+id/image5"
                android:layout_below="@+id/image5"
                android:layout_marginTop="18dp"
                app:srcCompat="@drawable/ic_launcher_background" />

            <ImageView
                android:id="@+id/image5"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="32dp"
                android:layout_marginTop="39dp"
                app:srcCompat="@drawable/ic_launcher_background" />

            <ImageView
                android:id="@+id/image6"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_alignLeft="@+id/image4"
                android:layout_alignTop="@+id/image8"
                app:srcCompat="@drawable/ic_launcher_background" />

            <ImageView
                android:id="@+id/image8"
                android:layout_width="140dp"
                android:layout_height="140dp"
                android:layout_alignLeft="@+id/image3"
                android:layout_below="@+id/image3"
                android:layout_marginTop="27dp"
                app:srcCompat="@drawable/ic_launcher_background" />

            <android.support.design.widget.FloatingActionButton
                android:id="@+id/floatingActionButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="21dp"
                android:clickable="true"
                app:srcCompat="@android:drawable/stat_notify_more" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>